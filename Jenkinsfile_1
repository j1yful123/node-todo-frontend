pipeline {
  agent any
    
  tools {nodejs "node11"}  // This is the Installation name of nodejs on your jenkins instance
    
  stages {
        
    stage('Cloning Git') {
      steps {
        git 'https://github.com/j1yful123/node-todo-frontend.git'
      }
    }
        
    stage('Install dependencies') {
      steps {
        sh 'npm install'
      }
    }
     
    stage('Test') {
      steps {
         sh 'npm test'
      }
    } 
   stage('aws'){
       steps{
    withAWS(credentials: 's3-jay-upload', region: 'us-east-1') {
    s3Upload bucket: 'j1yful-test', excludePathPattern: '.git', includePathPattern: '**/*', path: '${JOB_NAME}-${BUILD_NUMBER}', workingDir: '.'
          }
       }
    }
  }
}
